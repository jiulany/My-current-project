
.tab-bar {
  position: fixed;
  bottom: 0;
  background: rgba(255, 255, 255, 1);
  border-top: 1px solid #f5f5f5;
  padding: 13rpx 0;
  padding-bottom: 4rpx
}
.tab-bar-tt {
  -webkit-box-pack: center;
  -webkit-justify-content: center;
      -ms-flex-pack: center;
          justify-content: center;
  font-size: 25rpx;
  color: #fdd000;
}
