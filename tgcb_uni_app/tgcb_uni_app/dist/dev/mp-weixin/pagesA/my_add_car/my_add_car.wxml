<view class="span24 addcar"><view data-event-opts="{{[['tap',[['_chooseBrand',['$event']]]]]}}" class="span24 addcar-it" bindtap="__e"><view class="span24 addcar-tt">汽车品牌</view><view class="span24 addcar-inp"><view>{{!choice_brand?'请选择汽车品牌':choice_brand}}</view></view></view><view class="span24 addcar-it"><view class="span24 addcar-tt">车辆车牌</view><view class="span24 addcar-inp inpcar"><input type="text" placeholder="请输入车辆车牌" value="{{car_num}}"/><view data-event-opts="{{[['tap',[['openKeyboard',['$event']]]]]}}" class="keyblur" bindtap="__e"></view></view></view><view class="span24 addcar-it"><view class="span24 addcar-tt">车辆颜色</view><view data-event-opts="{{[['tap',[['getColor',['$event']]]]]}}" class="span24 addcar-inp" bindtap="__e"><view class="color:#e1e1e1">{{!carColor?'请选择汽车颜色':carColor}}</view><block wx:if="{{colorVisible}}"><view class="addcar-sle"><block wx:for="{{colorArr}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['choiseColor',['$0'],[[['colorArr','',index]]]]]]]}}" class="span24 addcar-sle-it" catchtap="__e">{{item}}</view></block></view></block></view></view><view class="span24 addcar-it"><view class="span24 addcar-tt">车辆类型</view><view data-event-opts="{{[['tap',[['getType',['$event']]]]]}}" class="span24 addcar-inp" bindtap="__e"><view class="color:#e1e1e1">{{!carTypeString?'请选择汽车类型':carTypeString}}</view><block wx:if="{{carTypeVisible}}"><view class="addcar-sle"><block wx:for="{{typeArr}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['choiseType',['$0'],[[['typeArr','',index]]]]]]]}}" class="span24 addcar-sle-it" catchtap="__e">{{item}}</view></block></view></block></view></view><view class="span24 addcar-save"><view data-event-opts="{{[['tap',[['save',['$event']]]]]}}" bindtap="__e">保存</view></view><keyboader vue-id="1" visible="{{show_keyboader}}" data-event-opts="{{[['^changeValue',[['changeValue']]],['^close',[['closeKeyboard']]]]}}" bind:changeValue="__e" bind:close="__e" bind:__l="__l"></keyboader></view>