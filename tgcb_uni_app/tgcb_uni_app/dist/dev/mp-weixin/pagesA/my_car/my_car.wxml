<view class="span24 mycar"><block wx:if="{{!type}}"><view data-event-opts="{{[['tap',[['car_check',['$event']]]]]}}" class="span24 mycar-lj" bindtap="__e"><view class="span2 mycar-lj-ico"><image mode="aspectFit" src="../../static/img/my_cheliang.png"></image></view><view class="span20 mycar-lj-tt">车辆年检查询</view><view class="span2 mycar-lj-rtr"><view class="iconfont icon-xiayibu"></view></view></view></block><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['choice',['$0'],[[['list','',index]]]]]]]}}" class="car_content" bindtap="__e"><view class="car_header">{{item.car_num}}</view><view class="car_content_box"><image src="{{item.vehicle.brand.url}}"></image><view class="car_title">{{''+item.sales_name+''}}</view><block wx:if="{{!type}}"><view data-event-opts="{{[['tap',[['edit',['$0'],[[['list','',index,'id']]]]]]]}}" class="car_content_right" bindtap="__e">编辑</view></block></view><block wx:if="{{!type}}"><view class="car_bottom"><view class="radios_box"><radio class="radios" color="#FDCE33"></radio><label class="_span">设为默认</label></view><view><view data-event-opts="{{[['tap',[['deleteCar',['$0',index],[[['list','',index,'id']]]]]]]}}" class="iconfont icon-lajitong  _i" style="font-size:50rpx;" bindtap="__e"></view></view></view></block></view></block><view data-event-opts="{{[['tap',[['toAddCar',['$event']]]]]}}" class="span24 car_add" bindtap="__e">+ 添加车辆</view></view>