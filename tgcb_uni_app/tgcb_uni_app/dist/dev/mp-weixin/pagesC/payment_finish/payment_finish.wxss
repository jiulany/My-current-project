.payment_finish {
  background: #ffffff;
  color: #666666;
  font-size: 30rpx;
  height: 100vh;
}
.payment_finish .order-sn {
  padding: 15rpx 28rpx;
}
.payment_finish .logo {
  width: 163rpx;
  height: 163rpx;
  margin: 50rpx auto 10rpx auto;
}
.payment_finish .logo image {
  width: 100%;
  height: 100%;
}
.payment_finish .payment-success {
  color: #FDD000;
  font-size: 34rpx;
  text-align: center;
}
.payment_finish .main {
  padding: 40rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
      -ms-flex-pack: center;
          justify-content: center;
}
.payment_finish .main .payment-text-box {
  display: inline-block;
}
.payment_finish .main .payment-text-box .payment-text {
  color: #666666;
  font-size: 32rpx;
}
.payment_finish .bottom-box {
  padding: 20rpx 66rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
      -ms-flex-pack: justify;
          justify-content: space-between;
}
.payment_finish .bottom-box view {
  width: 249rpx;
  height: 73rpx;
  border-radius: 37rpx;
  text-align: center;
  line-height: 73rpx;
}
.payment_finish .bottom-box .check-order {
  background: #e7e7e7;
}
.payment_finish .bottom-box .back-index {
  background: #FDD000;
}

