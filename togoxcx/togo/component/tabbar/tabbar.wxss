@media only screen and (device-width: 375px) and (device-height: 812px) and (-webkit-device-pixel-ratio: 3) {
    .tabbar_box {
      padding-bottom: 34px !important;
    }
}

.tabbar_box{
  display: flex;z-index: 200001;
  justify-content: space-around;
  position: fixed;
  /* height: 100rpx; */
  width: 100%;
  bottom: 0;
  left: 0;
  /* border-top: 1px solid #a9abac; */
  box-sizing: border-box;
  padding-top: 18rpx;
}
.tabbar_box text{
  font-size: 13px;
  color: #999 ;
}
.tabbar_box .icon{
  font-size: 45rpx;
  width: 45rpx;
  height: 45rpx;
  
}
.tabbar_box .tabbar_nav{
  display: flex;
  flex-direction: column;
  align-items: center;
  height: 100%;
}
.tabbar_box .tabbar_nav:nth-child(3) image{
  position: absolute;
  top: -30rpx;
  /* z-index: 100; */
  width: 76rpx;
  height: 76rpx;
}
.tabbar_box .tabbar_nav:nth-child(3) text{
  position: absolute;
  top: 56rpx;
  /* z-index: 100; */
}

