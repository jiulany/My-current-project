/* pages/cash_car/cash_car.wxss */

page {
  background: #f5f5f5;
}

.casr_itm {
  padding: 40rpx 25rpx 0 25rpx;
  width: 100%;
}

.casr_tt {
  margin-bottom: 10rpx;
}

.casr_time {
  display: flex;
  justify-content: space-between;
  width: 100%;
}

.casr_inp {
  background: white;
  padding: 30rpx 20rpx;
  position: relative;
  display: inline-block;
  padding-right: 80rpx;
  font-size: 14px;
}

.casr_inp image {
  width: 30rpx;
  height: 20rpx;
  position: absolute;
  right: 20rpx;
  top: 45rpx;
}

.casr_inp text {
  position: absolute;
  top: 33rpx;
  left: 20rpx;
  color: #a7a7a7;
}

.casr_inpt {
  width: 17.88888%;
  padding: 30rpx 20rpx;
  background: white;
  display: inline-block;
  position: relative;
  text-align: end;
  padding-right: 80rpx;
}

.casr_inpt image {
  width: 30rpx;
  height: 20rpx;
  position: absolute;
  right: 20rpx;
  top: 45rpx;
}

.casr_sm {
  text-align: center;
  padding: 80rpx 40rpx 0 40rpx;
  font-size: 14px;
  color: #a7a7a7;
  padding-bottom: 300rpx;
}

.casr_sm_tt {
  margin-bottom: 40rpx;
}
.casr_btn{
position: fixed;
bottom: 0;
background: #f5f5f5
}
.casr_btn button {
  color: white;
  background: #fdd555;
  border-radius: 0;
}
.casr_btn button:active{
   background: #fcd41a;
}
.casr_btn button::after {
  border: none
}
.zhez{
  width: 100%;
  height: 100%;
  z-index: 100;
  position: absolute;
  top: 0
}
.casr_up_rc{
text-align: center;
padding: 50rpx;
padding-bottom: 200rpx
}
.casr_up_rc image{
  height: 40rpx;
  width:350rpx
}