/* pages/my_car/my_car.wxss */

page {
  height: 100%;
  background: #f5f5f5;
  position: relative;
}

.all {
  padding-bottom: 120rpx;
}

.my_car_it {
  padding: 30rpx 50rpx;
  background: white;
  margin-top: 20rpx;
}

.my_car_it_sle {
  background: white;
}

.my_car_r1 {
  margin-bottom: 20rpx;
  font-size: 16px;
  font-weight: 600;
}

.my_car_cb image {
  width: 120rpx;
  max-height: 120rpx;
}

.my_car_ms {
  font-size: 15px;
  margin-top: 15rpx;
}

.my_car_gh {
  font-size: 15px;
  margin-top: 20rpx;
  text-align: end;
}

.my_car_r3 {
  margin-top: 50rpx;
  font-size: 15px;
  border-top: 1px solid #f5f5f5;
  padding-top: 30rpx;
}

.my_car_r3 text {
  position: relative;
  left: 18rpx;
  bottom: 6rpx;
}

.my_car_dl {
  text-align: end;
}

.my_car_dl image {
  width: 35rpx;
  height: 35rpx;
}

.my_mcar_2 {
  background: white;
  height: 100rpx;
  position: fixed;
  bottom: 0;
}

.btn_car {
  font-size: 15px;
  text-align: center;
  line-height: 100rpx;
  margin-left: 40%;
  color: #fdce33;
}

.no_car {
  text-align: center;
  position: absolute;
  top: 35%;
  font-size: 15px;
}

.no_car image {
  width: 80rpx;
  height: 80rpx;
}
