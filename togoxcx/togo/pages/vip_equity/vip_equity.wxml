      </i-col>
      <i-col span="24" i-class="data_toatal">
        {{ details.num }}人
      </i-col>
    </i-col>
    <i-col span="12">
      <i-col span="24" i-class="title_total">
        累计补贴金额已达
      </i-col>
      <i-col span="24" i-class="data_toatal">
        {{ details.money }}元
      </i-col>
    </i-col>
  </i-col>

  <i-col span="24" i-class="stop_car_all">
    <i-col span="7" i-class="stop_car_btn">
    {{ item.goodsname}}
    </i-col>
    <i-col span="17">
    </i-col>
    <i-col span="24">
      <i-col span="11" i-class="style_number">
